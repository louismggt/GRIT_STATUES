<%= simple_form_for [@statue, @rental] do |f| %>
  <%= f.input :start_date, as: :date, html5: true  %>
  <%= f.input :end_date, as: :date, html5: true  %>
  <%= f.input :description, label: 'Leave a message to the sharer!' %>
  <%= f.submit class: 'btn btn-primary' %>
  <%= link_to 'Go back to homepage', root_path, class:'btn btn-outline-primary' %>
<% end %>
